from quran_data where r5_2 is null 
and (tags like '%,farsh,%' or tags like '%2hamz%' or tags like '%1hamz%') 
and 
resultnew is null
order by sub_subject1,reading</sql><sql name="SQL 2"></sql><sql name="SQL 7">WITH R AS (
    SELECT reading, tags
    FROM tagsrevise
    WHERE done IS NOT NULL
)
UPDATE quran_data
SET done = 1
FROM R
WHERE quran_data.reading = R.reading AND quran_data.tags = R.tags;</sql><sql name="SQL 8">DELETE from tagsrevise where done =1</sql><current_tab id="0"/></tab_sql></sqlb_project>
