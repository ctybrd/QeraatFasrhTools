  quran_data

where done is null
and
r5_2 is null
group by reading
ORDER by count(*) desc</sql><sql name="SQL 3">UPDATE quran_data set done=1,tags=IFNULL(tags,',') ||',2hamz2,'
where reading 
IN ('بتحقيق الهمزة الأولى، مع تسهيل الهمزة الثانية، وله الإبدال مع الإشباع.'

)
and ifnull(tags,',') not like '%,2hamz2,%';
SELECT count(*) from quran_data where done is null;</sql><sql name="SQL 5">SELECT count(*) from quran_data where done is null;</sql><sql name="SQL 6">UPDATE quran_data SET resultnew =
substring(sub_subject1,1,2) ||
'●' ||substring(sub_subject1,4,100)
 ,tags=',2hamz1,'
 WHERE
reading ='بهمزتين على الاستفهام، مع تسهيل الثانية، بلا إدخال..'
 and done is null
and
r5_2 is null
</sql><sql name="SQL 10">UPDATE quran_data 
SET done=1,  resultnew = CASE
    WHEN sub_subject = 'فيغرقكم' THEN 'فتغرقكم'
    WHEN sub_subject = 'ينال' THEN 'تنال'
    WHEN sub_subject = 'يناله' THEN 'تناله'
    WHEN sub_subject = 'يكن' THEN 'تكن'
    WHEN sub_subject = 'يكون' THEN 'تكون'
    WHEN sub_subject = 'يحل' THEN 'تحل'
    WHEN sub_subject = 'ينفع' THEN 'تنفع'
    WHEN sub_subject = 'يغلي' THEN 'تغلي'
    WHEN sub_subject = 'يزيغ' THEN 'تزيغ'
END
WHERE 
reading ='بتاء التأنيث.'
 and done is null
and
r5_2 is null;

SELECT count(*) from quran_data where done is null;</sql><sql name="SQL 8">select  reading,count(*) from quran_data where resultnew is not null and done is null

group by reading
order by count(*) desc</sql><sql name="SQL 8">SELECT ROW_NUMBER() OVER (ORDER BY COUNT(*) DESC) AS Sequence,reading, count(*) Count, group_concat(DISTINCT sub_subject) Subject, group_concat(DISTINCT qarees) qarees FROM quran_data  where  r5_2 IS NULL GROUP BY reading  ORDER BY count(*) DESC </sql><sql name="SQL 9"></sql><current_tab id="2"/></tab_sql></sqlb_project>
